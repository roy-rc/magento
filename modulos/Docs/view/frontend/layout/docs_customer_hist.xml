<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <title>Historial de compras</title>
        <css src="https://cdn.datatables.net/1.11.3/css/jquery.dataTables.min.css" src_type="url" />
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Documents Hist</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="hist" template="Customcode_Docs::hist.phtml" cacheable="false">
            </block>
        </referenceContainer>
    </body>
</page>